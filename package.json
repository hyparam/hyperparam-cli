{
  "name": "hyperparam",
  "version": "0.2.23",
  "description": "Hyperparam CLI",
  "author": "Hyperparam",
  "homepage": "https://hyperparam.app",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyparam/hyperparam-cli.git"
  },
  "type": "module",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./dist/index.min.js"
    },
    "./components.css": "./dist/components.css"
  },
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "coverage": "vitest run --coverage --coverage.include=src --coverage.include=bin",
    "dev:vite": "vite",
    "dev:serve": "npm run serve",
    "dev:url": "npm run dev:serve -- https://hyperparam.blob.core.windows.net/hyperparam/starcoderdata-js-00000-of-00065.parquet",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "preview:vite": "vite preview",
    "serve": "node bin/cli.js",
    "preserve": "npm run build",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "hightable": "0.11.0",
    "hyparquet": "1.8.4",
    "hyparquet-compressors": "1.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@testing-library/react": "16.2.0",
    "@types/node": "22.13.5",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@vitejs/plugin-react": "4.3.4",
    "@vitest/coverage-v8": "3.0.7",
    "eslint": "9.21.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "15.14.0",
    "jsdom": "26.0.0",
    "typescript": "5.7.3",
    "typescript-eslint": "8.25.0",
    "vite": "6.2.0",
    "vitest": "3.0.7"
  }
}
